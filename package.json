{
  "name": "bowser-engine",
  "version": "0.0.0",
  "description": "A WebGL game engine based on three.js.",
  "main": "index.js",
  "dependencies": {
    "pathfinding": "0.4.1",
    "watchjs": "0.0.0",
    "three": "0.58.1",
    "browserify": "2.8.1",
    "doxx": "0.5.9",
    "hexa-random": "0.0.1"
  },
  "scripts": {
    "document": "rm -rf docs & doxx --source src --target docs",
    "build": "if [ ! -d \"build\" ]; then mkdir build; fi && browserify index.js > build/bowser-engine.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bowserjs/bowser-engine.git"
  },
  "keywords": [
    "game",
    "webgl",
    "pixel",
    "three",
    "engine",
    "3d",
    "2d"
  ],
  "author": "Bowser.js",
  "homepage": "http://www.bowserjs.org",
  "license": "BSD"
}
